    "form-data": "^4.0.0",
    "json2csv": "^6.0.0-alpha.2",
    "chalk": "^4.1.2",
    "sharp": "^0.33.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "crypto-js": "^4.2.0",
    "exceljs": "^4.4.0",
    "html-pdf-node": "^1.0.8"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  }
}
