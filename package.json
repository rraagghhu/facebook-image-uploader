{
  "name": "facebook-image-uploader",
  "version": "1.0.0",
  "description": "Upload images to Facebook Ads and generate CSV with image hashes",
  "main": "upload.js",
  "bin": "upload.js",
  "pkg": {
    "targets": [
      "node18-macos-x64",
      "node18-linux-x64",
      "node18-win-x64"
    ],
    "outputPath": "dist"
  },
  "scripts": {
